<ResultScreen>:
    MDScrollView:
        do_scroll_x:False
        do_scroll_y:True
        MDBoxLayout:
            id:mybox
            orientation: 'vertical'
            size_hint_y: 0.9
            pos_hint:{"center_y":0.8}
            padding:10
            spacing:10
            Image:
                id:image
                resolution:(640,480)
            Image:
                id:image2
                source: 'images/n.png'
            MDTextField:
                id:name2
                hint_text:"Nombre"
                font_size: 30
            MDRaisedButton:
                id:name3
                text:"Volver"
                on_release:root.captureyouface()