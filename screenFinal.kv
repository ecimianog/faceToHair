<FinalScreen>:
    MDScrollView:
        do_scroll_x:False
        do_scroll_y:True
        MDBoxLayout:
            id:mybox
            orientation: 'vertical'
            size_hint_y: 0.9
            pos_hint:{"center_y":0.8}
            padding:10
            spacing:10
            Image:
                id:imageF
                resolution:(640,480)
            MDTextField:
                id:name2
                hint_text:"Nombre"
                font_size: 30
            MDRaisedButton:
                id: btnSave
                text: "Guardar"
                on_press: app.stop() 
            MDRaisedButton:
                id: btnExit
                text: "Exit"
                on_press: app.stop() 