<Homescreen>:
    MDScrollView:
        do_scroll_x:False
        do_scroll_y:True
        MDBoxLayout:
            id:mybox
            orientation: 'vertical'
            size_hint_y: 0.9
            pos_hint:{"center_y":0.8}
            padding:10
            spacing:10
            Camera:
                id:camera
                play:True
            MDTextField:
                id:name
                hint_text:"Nombre"
                font_size: 30
            MDRaisedButton:
                text:"Env√≠a"
                on_release:root.sendImage()
            MDList:
                id:resultbox
            MDRaisedButton:
                id: btnExit
                text: "Salir"
                on_press: app.stop() 
           



